programa
{
	
	funcao inicio()
	{
		real nota
		escreva("Qual a Nota? ")
		leia(nota)
		se(nota>=7.0){
			escreva("Aprovado")
		}
		senao{
			escreva("Reprovado")
		}
	}
}
